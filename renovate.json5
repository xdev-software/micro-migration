{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "rebaseWhen": "behind-base-branch",
  "packageRules": [
    {
      "description": "Ignore MicroStream < 8",
      "matchPackagePatterns": [
        "^one.microstream"
      ],
      "datasources": [
        "maven"
      ],
      "matchCurrentVersion": "<8",
      "enabled": false
    }
  ]
}
